		"@sveltejs/adapter-netlify": "^4.1.0",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/eslint": "8.56.0",
		"@types/node": "^20.11.5",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"caml-mkdn": "^0.0.2",
		"del-cli": "^5.1.0",
		"eslint": "^8.56.0",
